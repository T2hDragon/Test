@page
@using Resources.Base.Areas.Identity.Pages.Account
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = ResetPasswordConfirmation.ResetPasswordConfirm;
}
<div class="jumbotron shadow rounded">

    <h1>@ResetPasswordConfirmation.ResetPasswordConfirm</h1>
    <p>
        @ResetPasswordConfirmation.PasswordHasBeenResetPlease<a asp-page="./Login">@ResetPasswordConfirmation.ClickHereToLogIn</a>.
    </p>
</div>